{
	"name": "joomla/string",
	"type": "joomla-package",
	"description": "Joomla String Package",
	"keywords": [
		"joomla",
		"framework",
		"string"
	],
	"homepage": "https://github.com/joomla-framework/string",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^7.2.5",
		"symfony/deprecation-contracts": "^2.1",
		"voku/portable-utf8": "^4.0|^5.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"doctrine/inflector": "1.2",
		"joomla/coding-standards": "^3.0@dev",
		"joomla/test": "^2.0",
		"phpunit/phpunit": "^8.5|^9.0"
	},
	"conflict": {
		"doctrine/inflector": "<1.2"
	},
	"replace": {
		"symfony/polyfill-php72": "1.99",
		"symfony/polyfill-iconv": "1.99",
		"symfony/polyfill-intl-grapheme": "1.99",
		"symfony/polyfill-intl-normalizer": "1.99",
		"symfony/polyfill-mbstring": "1.99"
	},
	"suggest": {
		"ext-mbstring": "For improved performance",
		"ext-iconv": "For improved performance",
		"doctrine/inflector": "To use the string inflector"
	},
	"autoload": {
		"psr-4": {
			"Joomla\\String\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Joomla\\String\\Tests\\": "Tests/"
		}
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-2.0-dev": "2.0-dev"
		}
	}
}
